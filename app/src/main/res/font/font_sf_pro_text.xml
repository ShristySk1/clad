        app:font="@font/sf_pro_text_regular_italic"/>
    <font
        android:font="@font/sf_pro_text_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/sf_pro_text_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>